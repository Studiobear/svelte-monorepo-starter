{
  "name": "core",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "build": "yarn transcribe && rollup -c",
    "dev": "concurrently \"yarn transcribe:watch\" \"rollup -c -w\"",
    "start": "sirv public",
    "clean:svelte": "ts-node ./script/clean-svelte.ts",
    "transcribe": "sd ./src",
    "transcribe:watch": "sd ./src -w",
    "lint": "prettier --check 'src/**/*.{html,js,svelte}'",
    "lint:fix": "prettier --write 'src/**/*.{html,js,svelte}'"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@testing-library/svelte": "^1.11.0",
    "concurrently": "^5.1.0",
    "csstype": "^2.6.8",
    "filewalker": "^0.1.3",
    "fs-extra": "^8.1.0",
    "jest": "^25.1.0",
    "jest-dom": "^4.0.0",
    "prettier": "^1.19.1",
    "prettier-plugin-svelte": "^0.7.0",
    "rollup": "^1.21.4",
    "rollup-plugin-livereload": "^1.0.3",
    "rollup-plugin-svelte": "^5.1.0",
    "rollup-plugin-terser": "^5.1.2",
    "svelte": "^3.18.1",
    "svelte-draft": "^0.0.4",
    "svelte-jester": "^1.0.3",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "sirv-cli": "^0.4.4"
  }
}
